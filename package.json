{
  "name": "angular2-dashboard",
  "version": "1.0.0",
  "description": "Angular2 Admin Dashboard Template",
  "repository": {
    "type": "git",
    "url": "git+https://SmartBiz@github.com/smartbiz/angular2-dashboard.git"
  },
  "keywords": [
    "Angular2", "Angular2 Dashboard", "Angular2 Admin Dashboard", "Angular2 Admin Dashboard Template", "Dashboard", "Admin Dashboard", "Admin Dashboard Template"
  ],
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "postinstall": "typings install"
  },
